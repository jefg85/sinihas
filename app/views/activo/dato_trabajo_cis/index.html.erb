<div>
      <div class="content-box">
        <div class="content-header"><h3>
        <div class="title-box">
               Dato Trabajo  
        </div>  
        <div class="btns-box">
          <button onclick="window.location='/seguridad/usuarios/new'">
                  Agregar
                </button>      
        </div></h3></div>
        <div class="content">
          <table class="table table-striped table-hover">			  
			  <tr>
			    <th>Tipo informacion trabajo</th>
			    <th>Fecha</th>
			    <th>Fuente</th>
			    <th>Resumen</th>			    
			    <th>Acciones</th>			    
			  </tr>			
			<% @activo_dato_trabajo_cis.each do |activo_dato_trabajo_ci| %>
			  <tr  class="<%= cycle("even", "odd") %>">
			    <td><%= activo_dato_trabajo_ci.tipo_informacion_trabajo_id %></td>
			    <td><%= activo_dato_trabajo_ci.fecha %></td>
			    <td><%= activo_dato_trabajo_ci.fuente_id %></td>
			    <td><%= activo_dato_trabajo_ci.resumen %></td>
			    <td><%= link_to 'Editar', edit_activo_dato_trabajo_ci_path(activo_dato_trabajo_ci) %> |
			    <%= link_to 'Borrar', activo_dato_trabajo_ci, method: :delete, data: { confirm: 'seguro de eliminar?' } %></td>
			  </tr>
			<% end %>
			</table>
        </div>
      </div>
</div>