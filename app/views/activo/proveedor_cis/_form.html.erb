    <td><%= f.text_field :telefono_convencional %></td>
  </tr>
    <tr>    
    <td><%= f.label :telefono_celular,"Celular:" %></td>
    <td> <%= f.text_field :telefono_celular %></td>
    <td><%= f.label :telefono_fax,"Fax:" %></td>
    <td> <%= f.text_field :telefono_fax %></td>
  </tr>
  <tr>   
    <td><%= f.label :fecha_inicio_vigencia,"Fecha Inicio Vigencia:" %></td>
    <td><%= f.text_field :fecha_inicio_vigencia %></td>
    <td><%= f.label :fecha_fin_vigencia,"Fecha Fin Vigencia" %></td>
    <td><%= f.text_field :fecha_fin_vigencia %></td>
  </tr>
  <tr>   
    <td><%= f.label :pagina_web,"Página Web:" %></td>
    <td><%= f.text_field :pagina_web %></td>
  </table>     
  <script>
    $(function() {
        $( "#activo_proveedor_ci_fecha_inicio_vigencia" ).datepicker();
        $( "#activo_proveedor_ci_fecha_fin_vigencia" ).datepicker();
    });
</script>
