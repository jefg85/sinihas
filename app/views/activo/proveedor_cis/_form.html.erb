<table class="form-table">
  <tr>
    <td><%= f.label :nit,"NIT:" %></td>
    <td><%= f.text_field :nit %></td>
    <td><%= f.label :nombre_comercial,"Nombre Comercial:" %></td>
    <td><%= f.text_field :nombre_comercial %></td>
  </tr>
    <tr>
    <td><%= f.label :sigla,"Sigla:" %></td>
    <td><%= f.text_field :sigla %></td>
    <td><%= f.label :tipo_proveedor_id,"Tipo:" %></td>
    <td><%= f.collection_select :tipo_proveedor_id, @tipo_proveedor_id, :id, :nombre,  {:include_blank => true}  %></td>
  </tr>
    <tr>
    <td><%= f.label :direccion,"Dirección:" %></td>
