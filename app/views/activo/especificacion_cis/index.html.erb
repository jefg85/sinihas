<div>
      <div class="content-box">
        <div class="content-header"><h3>
        <div class="title-box">
               Especificaciones  
        </div>  
        <div class="btns-box">
          <button onclick="window.location='/activo/especificacion_cis/new'">
                  Agregar
                </button>      
        </div></h3></div>
        <div class="content">
          <table class="table">			  
			  <tr>
			    <th>Ci</th>
			    <th>Prop esp ci</th>
			    <th>Valor cadena</th>
			    <th>Valor logico</th>
			    <th>Valor numerico</th>
			    <th>Valor real</th>			    
			    <th>Estado</th>
			    <th>Acciones</th>			    
			  </tr>
			
			<% @activo_especificacion_cis.each do |activo_especificacion_ci| %>
			  <tr  class="<%= cycle("even", "odd") %>">
			    <td><%= activo_especificacion_ci.ci_id %></td>
			    <td><%= activo_especificacion_ci.prop_esp_ci_id %></td>
			    <td><%= activo_especificacion_ci.valor_cadena %></td>
			    <td><%= activo_especificacion_ci.valor_logico %></td>
			    <td><%= activo_especificacion_ci.valor_numerico %></td>
			    <td><%= activo_especificacion_ci.valor_real %></td>			    
			    <td><%= activo_especificacion_ci.estado_id %></td>			    
			    <td><%= link_to 'Editar', edit_activo_especificacion_ci_path(activo_especificacion_ci) %> |
			    <%= link_to 'Borrar', activo_especificacion_ci, method: :delete, data: { confirm: 'Seguro de Eliminar?' } %></td>
			  </tr>
			<% end %>
			</table>

        </div>
      </div>
</div>